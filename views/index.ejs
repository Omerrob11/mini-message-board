<html>
  <body>
    <h1><%= title %></h1>
    <div class="messages-container">
      <% messages.forEach(message => { %>
      <div class="user">
        <p><%= message.text %></p>
        <p><%= message.user %></p>
        <p><%= message.added %></p>
      </div>
      <% }); %>
    </div>
  </body>
</html>
